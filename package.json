{
  "name": "agentic-ai",
  "title": "Agentic AI",
  "type": "module",
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "bun x --bun astro dev",
    "serv": "bun ./dist/server/entry.mjs",
    "build:dist": "bun x --bun astro build",
    "test": "bun x lint-staged && bun x commitlint",
    "prepare": "husky",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build:dist && electron-builder --win --config",
    "build:mac": "npm run build:dist && electron-builder --mac --config",
    "build:linux": "npm run build:dist && electron-builder --linux --config"
  },
  "lint-staged": {
    "*": "prettier --check . --write",
    "*.{js,mjs,svelte,astro}": "eslint --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@astrojs/node": "^9.5.1",
    "@astrojs/svelte": "^7.2.4",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.39.2",
    "@nurodev/astro-bun": "^2.1.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/bun": "^1.3.5",
    "astro": "^5.16.6",
    "astro-electron": "^0.5.0",
    "astro-google-fonts-optimizer": "^0.2.2",
    "commitlint": "^20.2.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.0.12",
    "electron-log": "^5.4.3",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.46.1",
    "svelte-highlight": "^7.9.0",
    "tailwindcss": "^4.1.18"
  },
  "svelte": "dist/index.js",
  "private": true
}
